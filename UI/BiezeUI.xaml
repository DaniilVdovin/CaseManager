<UserControl x:Class="CaseManager.BiezeUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CaseManager"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="300" Height="300">
    <Grid>
        <Line X1="7" X2="293" Y1="7" Y2="293" Stroke="#FFED0000" IsEnabled="False" IsHitTestVisible="False" />
        <Line X1="150" X2="150" Y1="50" Y2="250" Stroke="#FFED0000" IsEnabled="False" IsHitTestVisible="False" />
        <Path Stroke="White" StrokeThickness="1"  
                Data="M 7,7 C 300,0 0,300 293,293" IsEnabled="False" IsHitTestVisible="False"/>
        <Ellipse Width="15" Height="15" Fill="WhiteSmoke" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Ellipse Width="15" Height="15" Fill="WhiteSmoke" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
    </Grid>
</UserControl>
