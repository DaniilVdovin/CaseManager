<UserControl x:Class="CaseManager.UI.AI.AI_LayerUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CaseManager.UI.AI"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="50" MinWidth="50">
    <Grid>
        <Border IsHitTestVisible="True" CornerRadius="2" BorderBrush="#FF878787" BorderThickness="1" Height="20" Margin="0" VerticalAlignment="Top" Width="50" Background="#FFC1BFBF"/>
        <StackPanel Margin="0,20,0,0" IsHitTestVisible="False" Width="50">
            <local:AI_NodeUI Margin="0,2,0,0" IsHitTestVisible="True"/>
            <local:AI_NodeUI Margin="0,2,0,0" IsHitTestVisible="True"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
            <local:AI_NodeUI Margin="0,2,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
